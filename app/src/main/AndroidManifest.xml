<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="me.yabbi.demo">

    <application
        android:fullBackupContent="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:networkSecurityConfig="@xml/network_security_config"
        android:theme="@style/Theme.Yabbiadsandroiddemo"
        tools:targetApi="n"
        android:allowBackup="true">
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <meta-data
            android:name = "YABBI_PUBLISHER_ID"
            android:value = "${YABBI_PUBLISHER_ID}"/>
        <meta-data
            android:name = "YABBI_INTERSTITIAL_ID"
            android:value = "${YABBI_INTERSTITIAL_ID}"/>
        <meta-data
            android:name = "YABBI_REWARDED_ID"
            android:value = "${YABBI_REWARDED_ID}"/>
        <meta-data
            android:name = "YANDEX_INTERSTITIAL_ID"
            android:value = "${YANDEX_INTERSTITIAL_ID}"/>
        <meta-data
            android:name = "YANDEX_REWARDED_ID"
            android:value = "${YANDEX_REWARDED_ID}"/>
        <meta-data
            android:name = "MINTEGRAL_APP_ID"
            android:value = "${MINTEGRAL_APP_ID}"/>
        <meta-data
            android:name = "MINTEGRAL_API_KEY"
            android:value = "${MINTEGRAL_API_KEY}"/>
        <meta-data
            android:name = "MINTEGRAL_INTERSTITIAL_PLACEMENT_ID"
            android:value = "${MINTEGRAL_INTERSTITIAL_PLACEMENT_ID}"/>
        <meta-data
            android:name = "MINTEGRAL_INTERSTITIAL_ID"
            android:value = "${MINTEGRAL_INTERSTITIAL_ID}"/>
        <meta-data
            android:name = "MINTEGRAL_REWARDED_PLACEMENT_ID"
            android:value = "${MINTEGRAL_REWARDED_PLACEMENT_ID}"/>
        <meta-data
            android:name = "MINTEGRAL_REWARDED_ID"
            android:value = "${MINTEGRAL_REWARDED_ID}"/>
    </application>
</manifest>